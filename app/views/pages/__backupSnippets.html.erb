// Header navigation
a.fi-widget {
  font-size: rem-calc(24);
  text-indent: rem-calc(16);
  width: $tabbar-menu-icon-width;
  height: $tabbar-menu-icon-height;
  color: $tabbar-menu-icon-color;
  transform: translate3d(0,0,0);

  &:link, &:visited {
    color: $tabbar-menu-icon-color;
  }
  
  &:hover, &:active {
    color: $tabbar-menu-icon-hover;
  }
}

