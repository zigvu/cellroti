<html>
<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">
  <title>How about now?</title>
  <link rel="stylesheet" media="screen" href="https://rawgithub.com/dc-js/dc.js/master/dc.css">
  <link rel="stylesheet" media="screen" href="http://cdn.foundation5.zurb.com/foundation.css">
</head>
<body>
  <script src="https://rawgithub.com/mbostock/d3/master/d3.js"></script>
  <script src="https://rawgithub.com/mtraynham/crossfilter/master/crossfilter.js"></script>
  <script src="https://rawgithub.com/dc-js/dc.js/master/dc.js"></script>
  <script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
  <script src="http://cdn.foundation5.zurb.com/foundation.js"></script>


  <h1>Testing!</h1>
  <div id="clicky" class="button">Clicky Clicky</div>
  <div id="test" class="dc-chart" />

  <!-- Help Sections -->
  <div id="dc-brand-effectiveness-series-chart-info-reveal" class="reveal-modal large" data-reveal>
    <h3>Brand Effectiveness Chart Help</h3>
    <p class="lead">Your couch.  It is mine.</p>
    <p>I'm a cool paragraph that lives inside of an even cooler modal. Wins!</p>
    <a class="close-reveal-modal">&#215;</a>
  </div>

  <script language="javascript" type="text/javascript">
    showDummyChart = function (experiments) {
    //////////////////////////////////////////////////////////////////
    // Test
    LOG_Date_Start = new Date();
    var chart = dc.seriesChart('#test');

    ndx2 = crossfilter(experiments);

    runDimension = ndx2.dimension(function (d) {
      return [+d.Run, +d.Expt];
    });
    runGroup = runDimension.group().reduceSum(function (d) {
      return +d.Speed;
    });
    chart.width(768)
      .height(480)
      .chart(function (c) {
        return dc.lineChart(c).interpolate('basis');
      })
      .x(d3.scale.linear().domain([0, 500]))
      .brushOn(false)
      .yAxisLabel("Measured Speed km/s")
      .xAxisLabel("Run")
      .clipPadding(10)
      .elasticY(true)
      .dimension(runDimension)
      .group(runGroup)
      .mouseZoomable(false)
      .seriesAccessor(function (d) {
        return "Expt: " + d.key[1];
      })
      .keyAccessor(function (d) {
        return +d.key[0];
      })
      .valueAccessor(function (d) {
        return +d.value;
      })
      .legend(dc.legend().x(350).y(350).itemHeight(13).gap(5).horizontal(1).legendWidth(140).itemWidth(70));
    chart.yAxis().tickFormat(function (d) {
      return d3.format(',d')(d);
    });
    chart.margins().left += 40;
    dc.renderAll();

    console.log("Time: Render time: " + ((new Date()).getTime() - LOG_Date_Start.getTime()) / 1000);

    //////////////////////////////////////////////////////////////////
    return 0;
  }

  var LOG_Date_Start = new Date();

  experiments = []
  var exptCounter = 1;
  var runCounter = 1;
  for (i = 0; i < 2000; i++) {
    experiments[i] = {
      'Expt': exptCounter,
      'Run': runCounter++,
      'Speed': Math.floor(Math.random() * 1000),
      'Speed2': Math.floor(Math.random() * 1000),
      'Speed12': Math.floor(Math.random() * 1000),
      'Speed22': Math.floor(Math.random() * 1000),
      'Speed32': Math.floor(Math.random() * 1000),
      'Speed42': Math.floor(Math.random() * 1000),
      'Speed52': Math.floor(Math.random() * 1000),
      'Speed62': Math.floor(Math.random() * 1000),
      'Speed72': Math.floor(Math.random() * 1000),
      'Speed82': Math.floor(Math.random() * 1000),
      'Speed92': Math.floor(Math.random() * 1000),
      'Speed21': Math.floor(Math.random() * 1000),
      'Speed23': Math.floor(Math.random() * 1000),
      'Speed24': Math.floor(Math.random() * 1000),
      'Speed25': Math.floor(Math.random() * 1000)
    };
    if (runCounter == 501) {
      exptCounter++;
      runCounter = 1;
    }
  }
  console.log("Time: Created data: " + ((new Date()).getTime() - LOG_Date_Start.getTime()) / 1000);
  LOG_Date_Start = new Date();

  $( document ).ready(function(){
    $(document).foundation(); 
    setTimeout(function () {
      console.log("Wait and render chart");
      showDummyChart(experiments);
    }, 0);
  });
</script>
</body>
</html>